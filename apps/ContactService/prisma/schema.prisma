datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Contact {
  createdAt DateTime @default(now())
  email     String?
  id        String   @id @default(cuid())
  message   String?
  name      String?
  updatedAt DateTime @updatedAt
}

model Slider {
  createdAt DateTime @default(now())
  id        String   @id @default(cuid())
  imageFile Json?
  imageUrl  String?
  link      String?
  title     String?
  updatedAt DateTime @updatedAt
}
