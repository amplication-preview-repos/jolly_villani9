datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Page {
  content     String?
  contentJson Json?
  contentOld  String?
  createdAt   DateTime @default(now())
  id          String   @id @default(cuid())
  slug        String?
  title       String?
  updatedAt   DateTime @updatedAt
}
